<?xml version='1.0' encoding='UTF-8'?>
<faces-config
    xmlns='http://java.sun.com/xml/ns/javaee'
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:schemaLocation='http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd'
    version='2.0'>

    <managed-bean>
	<managed-bean-name>userMBean</managed-bean-name>
	<managed-bean-class>cvpicker.managed.bean.UserManagedBean</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
	<managed-property>
	    <property-name>loginBean</property-name>
	    <value>#{loginMBean}</value>
	</managed-property>
    </managed-bean>
    <managed-bean>
	<managed-bean-name>loginMBean</managed-bean-name>
	<managed-bean-class>cvpicker.managed.bean.LoginManagedBean</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
	<managed-bean-name>autoCompleteUserMBean</managed-bean-name>
	<managed-bean-class>cvpicker.managed.bean.autoCompleteUserManagedBean</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
	<managed-bean-name>messageMBean</managed-bean-name>
	<managed-bean-class>cvpicker.managed.bean.MessageManagedBean</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
	<managed-property>
	    <property-name>loginBean</property-name>
	    <value>#{loginMBean}</value>
	</managed-property>
    </managed-bean>
    <managed-bean>
	<managed-bean-name>userConverter</managed-bean-name>
	<managed-bean-class>cvpicker.managed.bean.UserConverter</managed-bean-class>
	<managed-bean-scope>request</managed-bean-scope>
	<managed-property>
	    <property-name>userBean</property-name>
	    <value>#{userMBean}</value>
	</managed-property>
    </managed-bean>
    <managed-bean>
	<managed-bean-name>cropperMBean</managed-bean-name>
	<managed-bean-class>cvpicker.managed.bean.CropperManagedBean</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
	<managed-property>
	    <property-name>loginBean</property-name>
	    <value>#{loginMBean}</value>
	</managed-property>
    </managed-bean>
    <managed-bean>
	<managed-bean-name>friendMBean</managed-bean-name>
	<managed-bean-class>cvpicker.managed.bean.FriendManagedBean</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
	<managed-property>
	    <property-name>loginBean</property-name>
	    <value>#{loginMBean}</value>
	</managed-property>
	<managed-property>
	    <property-name>userBean</property-name>
	    <value>#{userMBean}</value>
	</managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>cvMBean</managed-bean-name>
        <managed-bean-class>cvpicker.managed.bean.CvManagedBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
	<managed-property>
	    <property-name>loginBean</property-name>
	    <value>#{loginMBean}</value>
	</managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>userSkillMBean</managed-bean-name>
        <managed-bean-class>cvpicker.managed.bean.UserSkillManagedBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
	<managed-property>
	    <property-name>loginBean</property-name>
	    <value>#{loginMBean}</value>
	</managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>sectionMBean</managed-bean-name>
        <managed-bean-class>cvpicker.managed.bean.SectionManagedBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
	<managed-property>
	    <property-name>loginBean</property-name>
	    <value>#{loginMBean}</value>
	</managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>searchMBean</managed-bean-name>
        <managed-bean-class>cvpicker.managed.bean.SearchManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
	<managed-property>
	    <property-name>loginBean</property-name>
	    <value>#{loginMBean}</value>
	</managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>privacyMBean</managed-bean-name>
        <managed-bean-class>cvpicker.managed.bean.PrivacyManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
	<managed-property>
	    <property-name>loginBean</property-name>
	    <value>#{loginMBean}</value>
	</managed-property>
	<managed-property>
	    <property-name>friendBean</property-name>
	    <value>#{friendMBean}</value>
	</managed-property>
    </managed-bean>
    <managed-bean>
	<managed-bean-name>privacyConverter</managed-bean-name>
	<managed-bean-class>cvpicker.managed.bean.PrivacyConverter</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
	<managed-property>
	    <property-name>privacyBean</property-name>
	    <value>#{privacyMBean}</value>
	</managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>appMBean</managed-bean-name>
        <managed-bean-class>cvpicker.managed.bean.AppManagedBean</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
</faces-config>